@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dg: <http://www.semanticweb.org/noesaffaf/ontologies/2021/6/dungeon#> .
@prefix dgg: <http://www.localhost:3030/alacDungeon?graph=> .

<Room1> {
    dgg:Room1 rdf:type dg:Room ;
               rdfs:label "Main Room" ;
               dg:hasConnection dgg:Door1_2 ;
               dg:hasConnection dgg:Door1_3 ;
               dg:hasItem dgg:SilverKey ;
    .
}

<Door1_2> {
    dgg:Door1_2 rdf:type dg:Door ;
                       rdfs:label "Door1_2" ;
                       dg:hasConnectedRoom dgg:Room1 ;
                       dg:hasConnectedRoom dgg:Room2 ;
                       dg:hasInteractibleKey dgg:SilverKey ;
    .
}

<Door1_3> {
    dgg:Door1_3 rdf:type dg:Door ;
                   rdfs:label "Door1_3" ;
                   dg:hasConnectedRoom dgg:Room1 ;
                   dg:hasConnectedRoom dgg:Room3 ;
                   dg:hasInteractibleKey dgg:SilverKey ;
    .
}

<Door3_4> {
    dgg:Door3_4 rdf:type dg:Door ;
                   rdfs:label "Door3_4" ;
                   dg:hasConnectedRoom dgg:Room3 ;
                   dg:hasConnectedRoom dgg:Room4 ;
                   dg:hasInteractibleKey dgg:GoldKey ;
    .
}

<SilverKey> {
    dgg:SilverKey rdf:type dg:Key ;
                   rdfs:label "Silver Key" ;
                   dg:hasInteractibleConnector dgg:Door1_2 ;
                   dg:hasInteractibleConnector dgg:Door1_3 ;
    .
}

<Room2> {
    dgg:Room2 rdf:type dg:Room ;
           rdfs:label "Inventory" ;
           dg:hasConnection dgg:Door1_2 ;
           dg:hasItem dgg:GoldenKey ;
    .
}

<GoldenKey> {
    dgg:GoldenKey rdf:type dgg:Key ;
               rdfs:label "Golden Key" ;
               dg:hasInteractibleConnector dgg:Door3_4 ;
    .
}

<Room3> {
    dgg:Room3 rdf:type dg:Room ;
               rdfs:label "Armory" ;
               dg:hasConnection dgg:Door1_3 ;
               dg:hasConnection dgg:Door3_4 ;
    .
}

<Room4> {
    dgg:Room4 rdf:type dg:Room ;
           rdfs:label "Treasure room" ;
           dg:hasConnection dgg:Door3_4 ;
    .
}

